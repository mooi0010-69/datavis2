{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 800,
  "height": 800,
  "data": {
    "url": "https://raw.githubusercontent.com/mooi0010-69/datavis2/refs/heads/main/cleaned_data/map.json",
    "format": {
      "type": "topojson",
      "feature": "LGA_2023_AUST_GDA94"
    }
  },
  "transform": [
    {
      "lookup": "properties.STE_CODE21",
      "from": {
        "data": {
          "url": "https://raw.githubusercontent.com/mooi0010-69/datavis2/refs/heads/main/cleaned_data/total_pop.csv"
        },
        "key": "STATE_CODE",
        "fields": ["Population"]
      }
    },
    {
      "lookup": "properties.STE_CODE21",
      "from": {
        "data": {
          "url": "https://raw.githubusercontent.com/mooi0010-69/datavis2/refs/heads/main/cleaned_data/female_population.csv"
        },
        "key": "STATE_CODE",
        "fields": ["Female_Population"]
      }
    },
    {
      "lookup": "properties.STE_CODE21",
      "from": {
        "data": {
          "url": "https://raw.githubusercontent.com/mooi0010-69/datavis2/refs/heads/main/cleaned_data/male_pop.csv"
        },
        "key": "STATE_CODE",
        "fields": ["Male_Population"]
      }
    }
  ],
  "projection": {"type": "mercator"},
  "mark": {
    "type": "geoshape",
    "stroke": null
  },
  "encoding": {
    "color": {
      "field": "Population",
      "type": "quantitative",
      "scale": {"scheme": "blues"},
      "legend": {"title": "Total Population"}
    },
    "tooltip": [
      {"title": "State Name", "field": "properties.STE_NAME21", "type": "nominal"},
      {"title": "State Code", "field": "properties.STE_CODE21", "type": "nominal"},
      {"title": "Population", "field": "Population", "type": "quantitative", "format": ","},
      {"title": "Male Population", "field": "Male_Population", "type": "quantitative", "format": ","},
      {"title": "Female Population", "field": "Female_Population", "type": "quantitative", "format": ","}
    ]
  }
}
